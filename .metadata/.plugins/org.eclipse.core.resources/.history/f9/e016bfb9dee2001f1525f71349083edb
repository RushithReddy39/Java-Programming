
public class Main {
	
	public static void main(String[] args) {
		Book obj = new Book("Something ","Rushith ",148);
		Book obj2 = new Book("ashwant","boo");
		obj.title = "Some";
		obj.author = "Rushi ";
		obj.price = 184;
		obj2.display();
		obj.change(156);
		obj.display();
	}
	
	}
class Book{
	String title;
	String author;
	int price;
	public Book(String a,String b,int c) {
		title = a;
		author = b;
		price = c;
	}
	public Book(String a,String b) {
		title = a;
		author = b;
		price = 0;
	}
	public void display() {
		System.out.println(this.title+this.author+this.price);
	}
	public void change(int p) {
		this.price = p;
	}
}


