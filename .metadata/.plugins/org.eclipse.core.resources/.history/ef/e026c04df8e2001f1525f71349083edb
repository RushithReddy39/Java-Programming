
public class Main {

	public static void main(String[] args) {
		int[][] arr = new int[2][2];
		arr[0][0] = 5;
		arr[0][1] = 6;
		arr[1][0] = 1;
		arr[1][1] = 10;
		
		for(int i=0;i<arr.length;i++) {
			int c = 0;
			for(int j=0;j<arr.length;j++) {
				c = c+arr[i][j];
			}
			System.out.println(i + " row's sum is "+c);
		}
		
		int c1 = 0;
		int c2 = 0;
		for(int i=0;i<arr.length;i++) {
				c1 = c1+arr[0][i];
				c2 = c2+arr[1][i];
			}
			
		
		System.out.println(c1+" "+c2);
		//System.out.println(arr.length);
		
	}
	}



