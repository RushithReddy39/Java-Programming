
public class Main {

	public static void main(String[] args) {
		int[][] A = new int[2][];
		
		A[0] = new int[3];
		A[1] = new int[4];
		
		A[0][0] = 2;
		A[0][1] = 3;
		A[0][2] = 8;
		A[1][0] = 5;
		A[1][1] = 4;
		A[1][2] = 10;
		A[1][3] = 10;
		
		
		for(int i=0;i<A.length;i++) {
			for(int j=0;j<A[i].length;j++) {
			 System.out.println(A[i][j]);
			}
		}

	
	}
	}



